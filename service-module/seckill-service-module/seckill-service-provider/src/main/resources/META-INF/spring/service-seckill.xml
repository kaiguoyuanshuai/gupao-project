<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:service interface="com.gupao.edu.user.service.IUserLoginService"
                   ref="userLoginService" timeout="3000"/>

    <dubbo:service interface="com.gupao.edu.user.service.IJsonWebTokenService"
                   ref="jsonWebTokenService" timeout="3000"/>

    <dubbo:service interface="com.gupao.edu.user.service.IUserRegistryService"
                   ref="userRegistryService" timeout="3000"/>

    <dubbo:service interface="com.gupao.edu.user.service.IUserService"
                   ref="userService" timeout="3000"/>

</beans>